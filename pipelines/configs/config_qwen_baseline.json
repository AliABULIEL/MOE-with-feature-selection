{
    "model_id": "deepseek-ai/DeepSeek-V2-Lite",
    "model_name": "deepseek",
    "num_experts": 64,
    "default_top_k": 6,   
    
    "datasets": ["lambada", "hellaswag", "wikitext"],
    "max_samples": {
        "lambada": 500,
        "hellaswag": 500,
        "wikitext": 300
    },
    "max_length": 512,
    
    "kde_train_dataset": "lambada",
    "kde_test_datasets": ["hellaswag", "wikitext"],
    "kde_trim_amounts": [0, 1, 2, 4],
    "kde_kernels": ["gaussian", "tophat", "epanechnikov", "linear"],
    
    "output_dir": "./outputs/deepseek",
    
    "run_evaluation": true,
    "run_kde_training": true,
    "run_basic_plots": true,
    "run_per_expert_plots": false,
    "run_kde_plots": false,
    "run_pvalue_plots": false,
    
    "device": "cuda",
    "dtype": "bfloat16",
    
    "plot_format": "png",
    "plot_dpi": 100,
    "show_plots": false
}